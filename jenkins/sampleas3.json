{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.25.0",
        "id": "id_Test5",
        "label": "Test5",
        "remark": "An HTTP service with percentage based traffic distribution",
        "Test5": {
            "class": "Tenant",
            "App": {
                "class": "Application",
                "service": {
                    "class": "Service_L4",
                    "virtualAddresses": [
                        "192.0.2.200"
                    ],
                    "virtualPort": 80,
                    "persistenceMethods": [],
                    "profileL4": {
                        "bigip": "/Common/fastL4"
                    },
                    "snat": "auto",
                    "iRules": [
                        "Test5_bluegreen_irule"
                    ],
                    "pool": "green"
                },
                "blue": {
                    "class": "Pool",
                    "loadBalancingMode": "round-robin",
                    "members": [
                        {
                            "servicePort": 80,
                            "addressDiscovery": "event",
                            "shareNodes": true
                        }
                    ],
                    "monitors": [
                        {
                            "bigip": "/Common/tcp"
                        }
                    ]
                },
                "green": {
                    "class": "Pool",
                    "loadBalancingMode": "round-robin",
                    "members": [
                        {
                            "servicePort": 80,
                            "addressDiscovery": "event",
                            "shareNodes": true
                        }
                    ],
                    "monitors": [
                        {
                            "bigip": "/Common/tcp"
                        }
                    ]
                },
                "bluegreen_datagroup": {
                    "class": "Data_Group",
                    "remark": "BlueGreen Data Group for service",
                    "keyDataType": "string",
                    "records": [
                        {
                            "key": "distribution",
                            "value": "0.2"
                        },
                        {
                            "key": "blue_pool",
                            "value": "/Test5/App/blue"
                        },
                        {
                            "key": "green_pool",
                            "value": "/Test5/App/green"
                        }
                    ]
                },
                "Test5_bluegreen_irule": {
                    "class": "iRule",
                    "iRule": {
                        "base64": ""
                    }
                }
            }
        }
    }
}